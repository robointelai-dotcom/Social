{
  "name": "api",
  "version": "1.0.0",
  "description": "Api for socio manager project",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "concurrently \"esbuild src/index.ts --bundle --platform=node --outfile=dev/index.js --minify --sourcemap --watch\" \"nodemon dev/index.js\"",
    "build": "tsc --noEmit && esbuild src/index.ts --bundle --platform=node --minify --tree-shaking --outfile=dist/index.js",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "debug": "nodemon --inspect --exec ts-node src/index.ts"
  },
  "keywords": [
    "Socio Manager",
    "Api",
    "Typescript",
    "NodeJS",
    "Express"
  ],
  "author": "<contact@vamsi-k.com> Vamsi Konakanchi",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "node-cron": "^4.0.5",
    "nodemailer": "^6.9.16",
    "shared": "workspace:*",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.5.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.2",
    "eslint": "^8.40.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.0.4"
  }
}
